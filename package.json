		"happy-dom": "^8.1.0",
		"jsdom": "^20.0.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.19",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.2.4",
		"typescript": "~4.7.4",
		"vite": "^3.1.8",
		"vite-plugin-mkcert": "^1.10.1",
		"vitest": "^0.25.2",
		"vue-tsc": "^1.0.8"
	}
